<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_testingact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="8dp"
        android:enabled="false"
        android:text="Test(test)"
        app:layout_constraintBottom_toTopOf="@+id/btn_reviewupload"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_reviewupload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="12dp"
        android:text="Roulette List"
        app:layout_constraintBottom_toTopOf="@+id/btn_newlogin"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_myfavorites"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="myfav"
        app:layout_constraintBottom_toBottomOf="@+id/btn_newlogin"
        app:layout_constraintStart_toEndOf="@+id/btn_newlogin"
        app:layout_constraintTop_toTopOf="@+id/btn_newlogin" />

    <Button
        android:id="@+id/btn_newlogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="12dp"
        android:text="New Login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_loginact"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:background="@drawable/buttonst02"
        android:enabled="false"
        android:text="로그인 (Test)"
        android:textColor="@color/colorOrange1"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navihead_subtitle" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_temp2" />

    <TextView
        android:id="@+id/navihead_subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/testButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_list"
        app:layout_constraintVertical_bias="0.22000003" />

    <Button
        android:id="@+id/btn_favlist"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:background="@drawable/buttonst02"
        android:text="음식점 리스트"
        android:textColor="@color/colorOrange1"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_loginact"
        app:layout_constraintStart_toStartOf="@+id/btn_loginact"
        app:layout_constraintTop_toBottomOf="@+id/btn_loginact" />

    <Button
        android:id="@+id/btn_botnavi"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="12dp"
        android:text="bottom navi"
        app:layout_constraintBottom_toTopOf="@+id/btn_showmap"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn_showmap"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="12dp"
        android:text="showmap"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="list"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_roulette"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="132dp"
        android:text="roulette"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>